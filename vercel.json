{
  "outputDirectory": "build/web",
  "framework": null,
  "rewrites": [
    {
      "source": "/backend/(.*)",
      "destination": "http://69.62.77.235:8080/$1"
    },
    {
      "source": "/favicon.png",
      "destination": "/assets/images/MAQUEL_logo.png"
    },
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}